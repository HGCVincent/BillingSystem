<?xml version="1.0"?> 
<!DOCTYPE faces-config PUBLIC 
"-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 2.0//EN" 
"http://java.sun.com/dtd/web-facesconfig_2_0.dtd"> 
<faces-config> 
	<managed-bean> 
	    <managed-bean-name>invoiceListView</managed-bean-name> 
	    <managed-bean-class>com.billing.invoice.view.InvoiceListView</managed-bean-class> 
	    <managed-bean-scope>request</managed-bean-scope>
	    <managed-property>
	      <property-name>invoiceListService</property-name>
	      <value>#{invoiceListService}</value>
	    </managed-property>
	    <managed-property>
	      <property-name>invIssuingPartyService</property-name>
	      <value>#{issuingPartyService}</value>
	    </managed-property>
	    <managed-property>
	      <property-name>customerProfileService</property-name>
	      <value>#{customerProfileService}</value>
	    </managed-property>
	</managed-bean> 
	
	<managed-bean> 
	    <managed-bean-name>taxInvoiceNumView</managed-bean-name> 
	    <managed-bean-class>com.billing.invoice.view.TaxInvoiceNumView</managed-bean-class> 
	    <managed-bean-scope>request</managed-bean-scope>
	    <managed-property>
	      <property-name>taxInvoiceNumService</property-name>
	      <value>#{taxInvoiceNumService}</value>
	    </managed-property>
	</managed-bean>
</faces-config>
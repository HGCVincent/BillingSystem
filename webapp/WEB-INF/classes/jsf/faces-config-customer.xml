<?xml version="1.0"?> 
<!DOCTYPE faces-config PUBLIC 
"-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 2.0//EN" 
"http://java.sun.com/dtd/web-facesconfig_2_0.dtd"> 
<faces-config> 
	<managed-bean> 
	    <managed-bean-name>accountInfoView</managed-bean-name> 
	    <managed-bean-class>com.billing.invoice.view.AccountInfoView</managed-bean-class> 
	    <managed-bean-scope>request</managed-bean-scope>
	    <managed-property>
	      <property-name>accountInfoService</property-name>
	      <value>#{accountInfoService}</value>
	    </managed-property> 
	</managed-bean> 
	
	<managed-bean> 
	    <managed-bean-name>customerProfileView</managed-bean-name> 
	    <managed-bean-class>com.billing.invoice.view.CustomerProfileView</managed-bean-class> 
	    <managed-bean-scope>request</managed-bean-scope>
	    <managed-property>
	      <property-name>customerProfileService</property-name>
	      <value>#{customerProfileService}</value>
	    </managed-property> 
	</managed-bean>
</faces-config>